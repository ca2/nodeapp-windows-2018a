cp /c/third/static/x64/stage/lib/ossl_static.pdb /c/basis/nodeapp/x64/cover/lib/ossl_static.pdb

cp /c/third/static/x64/stage/lib/libcrypto.lib /c/basis/nodeapp/x64/stage/lib/_libcrypto.lib
cp /c/third/static/x64/stage/lib/libssl.lib /c/basis/nodeapp/x64/stage/lib/_libssl.lib
cp /c/third/static/x64/stage/lib/ossl_static.pdb /c/basis/nodeapp/x64/stage/lib/ossl_static.pdb

