cp /c/third/static/Win32/stage/lib/libcrypto.lib /c/basis/nodeapp/Win32/stage/lib/_libcrypto.lib
cp /c/third/static/Win32/stage/lib/libssl.lib /c/basis/nodeapp/Win32/stage/lib/_libssl.lib
